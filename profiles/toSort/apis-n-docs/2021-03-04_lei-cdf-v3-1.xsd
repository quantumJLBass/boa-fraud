<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:lei="http://www.gleif.org/data/schema/leidata/2016" targetNamespace="http://www.gleif.org/data/schema/leidata/2016" elementFormDefault="qualified">
